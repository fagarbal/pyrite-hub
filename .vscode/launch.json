{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch typescript",
        "env": {
            "NODE_PATH": "."
        },
        "protocol": "inspector",
        "args": ["./src/main.ts"],
        "cwd": "${workspaceFolder}/",
        "runtimeArgs": ["-r", "ts-node/register"],
        "internalConsoleOptions": "openOnSessionStart"
      }
    ]
  }