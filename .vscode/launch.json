{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "debug pyritehub",
        "env": {
            "NODE_ENV": "development",
            "TS_NODE_COMPILER_OPTIONS": "{\"module\":\"commonjs\"}"
        },
        "protocol": "inspector",
        "args": ["./server/server.ts"],
        "cwd": "${workspaceFolder}/",
        "runtimeArgs": ["-r", "ts-node/register"],
        "internalConsoleOptions": "openOnSessionStart"
      }
    ]
  }