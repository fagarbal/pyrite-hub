{
  "name": "pyrite-hub",
  "version": "0.0.1",
  "description": "Webcomponent Organization",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start:dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "analyze": "./node_modules/.bin/cross-env ANALYZE=1 npm run build",
    "dev": "npm run build && NODE_ENV=development node dist/main.js -p $PORT",
    "postbuild": "cp -r static .next/",
    "clean": "rm -rf .next/",
    "build": "npm run clean && next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/main.js -p $PORT",
    "test:debug": "./node_modules/.bin/jest --debug",
    "test:verbose": "./node_modules/.bin/jest --verbose",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:coverage": "npm test -- --coverage --no-cache",
    "test:coverage:view": "npm test -- --coverage --no-cache && open coverage/lcov-report/index.html",
    "tslint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
    "deploy": "time npm run deploy:heroku && npm run heroku:logs:tail",
    "deploy:force": "npm run deploy:heroku:force",
    "deploy:heroku": "git push heroku $(git rev-parse --abbrev-ref HEAD):master",
    "deploy:heroku:force": "git push -f heroku $(git rev-parse --abbrev-ref HEAD):master",
    "heroku-postbuild": "npm run build",
    "heroku:bash": "heroku run bash",
    "heroku:logs": "heroku logs",
    "heroku:logs:tail": "heroku logs -t",
    "docker:destroy": "docker kill $(docker ps -q)",
    "docker:nuke": "docker rmi -f $(docker images -q)",
    "docker:down": "docker-compose down",
    "docker:up": "docker-compose up -d && docker-compose logs -f",
    "docker:up:clean": "docker-compose up --build -d && docker-compose logs -f",
    "docker:up:timestamp": "docker-compose up -d && docker-compose logs -f -t",
    "docker:up:background": "docker-compose up -d"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/testing": "^4.5.5",
    "@nestjs/websockets": "^4.5.8",
    "@types/dotenv": "^4.0.3",
    "@zeit/next-typescript": "^0.1.1",
    "copy-webpack-plugin": "^4.5.1",
    "dotenv": "^5.0.1",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "handlebars-helper-repeat": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mysql": "^2.15.0",
    "next": "latest",
    "passport": "^0.4.0",
    "passport-bitbucket": "^2.0.0",
    "passport-bitbucket-oauth2": "^0.1.2",
    "passport-github": "^1.1.0",
    "passport-gitlab2": "^3.0.0",
    "react": "latest",
    "react-dom": "latest",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "ts-loader": "^3.5.0",
    "typeorm": "^0.2.0",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/next": "^2.4.9",
    "@types/node": "^9.6.2",
    "@types/passport": "^0.4.4",
    "@types/passport-github": "^1.1.3",
    "@types/supertest": "^2.0.4",
    "@zeit/next-source-maps": "0.0.2",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
